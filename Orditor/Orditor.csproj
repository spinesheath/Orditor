    <Resource Include="Data\pedestal.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Data\plant.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Data\sunstone.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
