    <Resource Include="Data\ec.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Data\gumonseal.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
