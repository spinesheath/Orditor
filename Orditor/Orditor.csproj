    <None Remove="Data\sunstone.png" />
    <None Remove="Data\tree.png" />
    <None Remove="Data\warmth.png" />
    <None Remove="Data\watervein.png" />
    <None Remove="Data\windrestored.png" />
    <None Remove="Data\world.png" />
    <None Remove="Data\xp.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AvalonEdit" Version="6.1.3.50" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Data\ac.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Data\cleanwater.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
